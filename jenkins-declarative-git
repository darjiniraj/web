currentBuild.displayName = "niraj-jenkins-demo-#"+currentBuild.number

pipeline {
   agent any

   stages {
      stage('Git Checkout') {
         steps {
            git credentialsId: 'git', url: 'https://github.com/darjiniraj/spring-boot-swagger'
         }
      }
	  
	  stage('Mvn Build') {
         steps {
            sh "mvn clean package"
         }
      }
   }
}

